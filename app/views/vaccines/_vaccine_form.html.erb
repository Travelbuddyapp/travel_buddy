

<%= form_for @vaccine do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :details %>
  <%= f.text_field :details %>

  <%= f.label :administration_method %>
  <%= f.text_field :administration_method %>

  <%= f.label :date_given %>
  <%= f.date_field :date_given %>

  <%= f.label :expiration_date %>
  <%= f.date_field :expiration_date %>

  <%= f.submit %>
<% end %>
